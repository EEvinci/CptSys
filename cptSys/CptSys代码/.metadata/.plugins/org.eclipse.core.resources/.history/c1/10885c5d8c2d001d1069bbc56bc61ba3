package cn.zucc.edu.models;

//组队信息表
public class BeanTeamUp {
	private int TeamUpNum;
	private String StuId;
	private String TeamId;
	private String CptId;
	private String IsLeader;
	private String IsPtcpt;
	
	public BeanTeamUp() {
		super();
		// TODO 自动生成的构造函数存根
	}
	
	public BeanTeamUp(int teamUpNum, String stuId, String teamId, String cptId, String isLeader, String isPtcpt) {
		super();
		TeamUpNum = teamUpNum;
		StuId = stuId;
		TeamId = teamId;
		CptId = cptId;
		IsLeader = isLeader;
		IsPtcpt = isPtcpt;
	}
	
	/*
	 * getter and setter
	 */
	public int getTeamUpNum() {
		return TeamUpNum;
	}
	public void setTeamUpNum(int teamUpNum) {
		TeamUpNum = teamUpNum;
	}
	public String getStuId() {
		return StuId;
	}
	public void setStuId(String stuId) {
		StuId = stuId;
	}
	public String getTeamId() {
		return TeamId;
	}
	public void setTeamId(String teamId) {
		TeamId = teamId;
	}
	public String getCptId() {
		return CptId;
	}
	public void setCptId(String cptId) {
		CptId = cptId;
	}
	public String getIsLeader() {
		return IsLeader;
	}
	public void setIsLeader(String isLeader) {
		IsLeader = isLeader;
	}
	public String getIsPtcpt() {
		return IsPtcpt;
	}
	public void setIsPtcpt(String isPtcpt) {
		IsPtcpt = isPtcpt;
	}	
	
	
}
